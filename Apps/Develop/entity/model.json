{
	"$schema": "../../../a2v10.web.runtime/app_application/@schemas/model-json-schema.json#",
	"schema": "a2demo",
	"model": "Entity",
	"dialogs": {
		"browseGoods": {
			"index": true,
			"model": "Goods",
			"view": "browseGoods.dialog"
		},
		"editGoods": {
			"view": "editGoods.dialog",
			"template": "editGoods.template",
			"checkTypes22": "edit.d"
		}
	},
	"commands": {
		"findArticle": {
			"type": "sql",
			"procedure": "Entity.FindArticle"
		},
		"duplicateArticle": {
			"type": "sql",
			"procedure": "Entity.Article.CheckDuplicate"
		}
	},
	"files": {
		"myfile": {
			"type": "sql",
			"container": "images",
			"model": "Entity.Image",
			"imageCompress": {
				"quality": 40,
				"threshold": 1
			}
		}
	}
}
