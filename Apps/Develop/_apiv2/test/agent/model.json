/*apiv2: test/agent*/
{
	"$schema": "../../../../../a2v10.web.runtime/app_application/@schemas/api-model-json-schema.json#",
	"schema": "a2demo",
	"commands": {
		"get": {
			"type": "sql",
			"method": "post",
			"clientId": [ "PrivatBank" ],
			"sqlCommand": {
				"action": "loadModel",
				"model": "Agent",
				"commandTimeout": 30,
				"parameters": {
					"Id": "id"
				},
				"returns": "Agent.Attachments"
			},
			"wrap": true
		},
		"update": {
			"type": "sql",
			"method": "post",
			"debugOnly": false,
			"sqlCommand": {
				"action": "executeSql",
				"procedure": "ExecuteApi",
				"parameters": {
					"Id": "id",
					"Name": "body.name",
					"Memo": "body.memo",
					"Config": "config.url"
				}
			}
		},
		"executeSql": {
			"type": "sql",
			"method": "get",
			"sqlCommand": {
				"action": "executeSql",
				"procedure": "Agent.Load",
				"parameters": {
					"Id": "id"
				},
				"returns": ""
			},
			"wrap": true
		},
		"userCreate": {
			"type": "sql",
			"method": "post",
			"sqlCommand": {
				"action": "executeSql",
				"procedure": "ExecuteApiFile",
				"parameters": {
					"Id": "id",
					"Name": "body.name",
					"File": "body.file",
					"Config": "config.url"
				}
			}
		},
		"script": {
			"type": "javascript",
			"javascriptCommand": {
				"script": "test",
				"parameters": {
					"x": "body.x",
					"s": "body.string"
				}
			}
		},
		"callClr": {
			"type": "clr",
			"clrCommand": {
				"clrType": "clr-type:MyClr;assembly=MyClr.Assembly"
			}
		},
		"create": {
			"type": "clr",
			"method": "post",
			"clrCommand": {
				"clrType": "clr-type:A2v10.Web.Mvc.Hooks.CreateTenantHandler;assembly=A2v10.Web.Mvc"
			}
		},
		"login": {
			"type": "clr",
			"method": "post",
			"clrCommand": {
				"clrType": "clr-type:A2v10.Web.Mvc.Hooks.LoginTenantUserHandler;assembly=A2v10.Web.Mvc"
			}
		}
	}
}
